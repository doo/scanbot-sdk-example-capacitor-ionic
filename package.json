{
  "name": "scanbot-example-sdk-capacitor",
  "version": "6.1.0",
  "author": "Scanbot SDK",
  "homepage": "https://scanbot.io",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration development && cd ios/App && pod install --repo-update",
    "sync": "npx cap sync",
    "android": "npx cap run android",
    "ios": "npx cap run ios",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "clean": "rm -f package-lock.json && rm -r -f www && cd ios/App && rm -r -f Pods && rm -f Podfile.lock && rm -r -f ../../node_modules"
  },
  "private": true,
  "dependencies": {
