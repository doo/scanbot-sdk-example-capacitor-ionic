{
  "name": "scanbot-example-sdk-capacitor",
  "version": "7.0.0",
  "author": "Scanbot SDK",
  "homepage": "https://scanbot.io",
  "description": "Scanbot SDK Example App for Capacitor with Angular",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration development && cd ios/App && pod install --repo-update",
    "sync": "npx cap sync",
    "android": "npx cap run android",
    "ios": "npx cap run ios",
    "watch": "ng build --watch --configuration development",
    "format": "prettier --write \"src/**/*.{ts,html,scss,json}\"",
    "clean": "rm -r -f node_modules && rm -f package-lock.json && rm -r -f www && cd ios/App && rm -r -f Pods && rm -f Podfile.lock"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/router": "^19.0.0",
    "@capacitor-community/file-opener": "7.0.1",
    "@capacitor/android": "7.2.0",
    "@capacitor/app": "7.0.1",
    "@capacitor/camera": "7.0.1",
    "@capacitor/core": "7.2.0",
    "@capacitor/filesystem": "7.0.1",
    "@capacitor/ios": "7.2.0",
    "@capacitor/status-bar": "7.0.1",
    "@capawesome/capacitor-file-picker": "7.0.1",
    "@ionic/angular": "^8.0.0",
    "capacitor-plugin-scanbot-sdk": "7.0.1-rc.1",
    "ionicons": "^7.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.0",
    "@angular/cli": "^19.0.0",
    "@angular/compiler-cli": "^19.0.0",
    "@angular/language-service": "^19.0.0",
    "@capacitor/cli": "7.2.0",
    "@ionic/angular-toolkit": "^12.0.0",
    "prettier": "^3.5.3",
    "typescript": "~5.6.3"
  },
  "engines": {
    "node": ">=20"
  },
  "browserslist": [
    "Chrome >=79",
    "ChromeAndroid >=79",
    "Firefox >=70",
    "Edge >=79",
    "Safari >=14",
    "iOS >=14"
  ]
}
